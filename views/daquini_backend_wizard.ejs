    <script>
        function imageExists(url, callback) {
            var img = new Image();
            img.onload = function () {
                callback(true);
            };
            img.onerror = function () {
                callback(false);
            };
            img.src = url;
        }
        var shopdata = <%-JSON.stringify(shopData[0])%>;
        var topPlaceholderImg = 'images/placeholder.png';
        var bottomPlaceholderImg = 'images/placeholder.png';
        imageExists('uploads/<%=settings[0].placeholderTopImage%>', function(exists) {
            if(exists){
                topPlaceholderImg = 'uploads/<%=settings[0].placeholderTopImage%>';
                $('#placeholder-image-top-window').css('background-image', 'url('+topPlaceholderImg+')');
            }
        });
        imageExists('uploads/<%=settings[0].placeholderBottomImage%>', function(exists) {
            if(exists){
                bottomPlaceholderImg = 'uploads/<%=settings[0].placeholderBottomImage%>';
                $('#placeholder-image-bottom-window').css('background-image', 'url('+bottomPlaceholderImg+')');
            }
        });
    </script>
